# Ligne pointillée de 10 pixels, puis 5 pixels vides, 5 pixels, 10 pixels vides

SYMBOL 
  NAME 'dashed_line1'
  TYPE ELLIPSE
  POINTS 1 1 END
  FILLED true
  STYLE 10 5 5 10 END
END

SYMBOL
  NAME 'dashed1'
  TYPE ELLIPSE
  POINTS 1 1 END
  FILLED TRUE
  STYLE 5 5 5 5 END
  END
END 


