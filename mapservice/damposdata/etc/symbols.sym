SYMBOL
  NAME "circle"
  TYPE ELLIPSE
  FILLED FALSE
  POINTS 1 1 END
END 

#-------------------------------------------
# To set line width in a polygon outline:
# Use a circle symbol as your CLASS in the layer. 
# Set the size of the circle to the thickness you want. 

SYMBOL
  NAME "line"
  TYPE ELLIPSE
  FILLED TRUE
  POINTS 1 1 END
END 

SYMBOL
  NAME "line1"
  TYPE ELLIPSE
  FILLED TRUE
  POINTS 1 1 END
END 


#-------------------------------------------
SYMBOL
  NAME "circle3px"
  TYPE ELLIPSE
  FILLED false
  POINTS 1 1 END
  STYLE 
    0 -1
    0 0
    0 1
  END 
END 



SYMBOL
  NAME "empty"
  TYPE ELLIPSE
  FILLED FALSE
  POINTS 0 0 END
END 




