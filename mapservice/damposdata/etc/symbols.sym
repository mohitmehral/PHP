SYMBOL
  NAME "star"
  TYPE VECTOR
  FILLED TRUE
  POINTS
    0 .375
    .35 .375
    .5 0
    .65 .375
    1 .375
    .75 .625
    .875 1
    .5 .75
    .125 1
    .25 .625
  END
END

